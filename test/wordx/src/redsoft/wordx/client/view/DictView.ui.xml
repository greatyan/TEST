<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<table>
			<col></col>
			<col></col>
			<tr>
				<td>
					<g:SuggestBox ui:field="headwordText" width="100%" height="100%"/>
				</td>
				<td style="padding-left:1em">
					<g:Image url="resources/star-white32.png" ui:field="reviewImage"/>
				</td>
			</tr>
			<tr>
				<td colspan="2">
					<g:HTML wordWrap="true" height="100%" width="100%" ui:field="explainFrame" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 